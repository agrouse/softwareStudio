<div id = "signupWrapper">
                <div class="box">
  
                        <div class="iconBack col-md-5" style="margin-top: 2%; margin-left:.5%">
                            <a href="/access/landing" style="color:white;"><span style="font-size: 30px;" class="glyphicon glyphicon-circle-arrow-left"></span></a>
                        </div>
                    

                            <h2 style="color:white; margin-top: 3%">Sign Up</h2>
                    

                        <%= form_for :people, :url => {:action => "create"}, validate: true do |f| %>
                            <div class="signupForm">
                                <div class = "row">
                                        <%= f.text_field :first_name, placeholder: "First name", :id => 'fname', :class =>'accessInput custom-control', :required => true%>
                                </div>
                                <div class = "row field">
                                    <%= f.text_field :last_name, placeholder: "Last name", :id => 'lname', :class =>'lname accessInput custom-control', :required => true%>
                                </div>
                                <div class = "row field">
                                    <%= f.text_field :username, placeholder: "User name", :id => 'uname', :class =>'uname accessInput custom-control', :required => true %>
                                </div>
                                <div class= "row field">
                                    <%= f.password_field :password, placeholder: "Password", :id => 'pword', :class => 'pword accessInput custom-control', :required => true %>
                                </div>
                                <div class="row field">
                                    <%= f.password_field :password_confirmation, placeholder: "Confirm password", :id => 'cpword',:class => 'cpword accessInput custom-control', :required => true %>
                                </div>
                                
                            <div 
                                <div class="row" style="margin-left: 40%; margin-top: 3%">
                                    <%= button_tag(type: 'submit', class: "submitBtn btn btn-primary") do %>
                                        <span class="glyphicon glyphicon-ok"></span> 
                                    <% end %>
                                </div>
                                
                                
  

                            </div>
                            <% end %>
                </div>
</div>
