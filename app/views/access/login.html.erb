<div id="signupWrapper">
    
    <div class="box">
        <div class="col-md-5">
            <a href="/access/landing" style="color:white;"><span style="font-size: 30px; margin-top: 5%;" class="glyphicon glyphicon-circle-arrow-left"></span></a>
        </div>
        <h2 style="color:white;text-align:center; margin-top: 5%">Login</h2>
        <%= form_tag(access_attempt_login_path, :class => 'regForm', :method => :post) do %>
            <%= label_tag(:username, nil, class: 'custom-control') %>
            <%= text_field_tag(:username, params[:username], class: 'custom-control') %>
            <%= label_tag(:password, nil, class: 'custom-control') %>
            <%= password_field_tag(:password, nil, class: 'custom-control') %>
            <%= button_tag(type: 'submit', value: "submit", class: "submitBtn btn btn-primary") do %>
                <span class="glyphicon glyphicon-ok"></span> 
            <% end %>
        <% end %>

    </div>
</div>