<!-- Log in form. Client side validation present with required: true-->
<%= label_tag(:username, nil, class: 'accessInput custom-control') %>
<%= text_field_tag(:username, params[:username], class: 'accessInput custom-control', required: true) %>
<%= label_tag(:password, nil, class: 'accessInput custom-control') %>
<%= password_field_tag(:password, nil, class: 'accessInput custom-control', required: true) %>
            
        